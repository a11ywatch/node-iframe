{
  "name": "node-iframe",
  "version": "1.7.1",
  "description": "create a iframe on your server to bypass CORS issues. ( reverse engineer security issues )",
  "main": "dist/iframe.js",
  "scripts": {
    "test": "jest",
    "prepare": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "keywords": [
    "node-iframe",
    "node-iframe-cors",
    "node-iframe-hack",
    "iframe"
  ],
  "author": "Jeff Mendez",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "isomorphic-unfetch": "^3.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.165",
    "@swc/jest": "^0.2.20",
    "@types/cheerio": "^0.22.31",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "tscpaths": "0.0.9",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
